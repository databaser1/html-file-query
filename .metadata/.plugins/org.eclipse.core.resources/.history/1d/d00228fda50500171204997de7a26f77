var app = angular.module('app', []);

app.config(
  [
    '$controllerProvider', 
    function($controllerProvider) {
      $controllerProvider.allowGlobals();
    }
  ]
);

app.value('url', '');

app.controller(
  'planReaderController', 
  function($scope, url) {
    var date = new Date();
    var dateStr = date.toJSON().slice(0,10);
    $('#date-picker').val(dateStr);
  }
  
  $scope.
);